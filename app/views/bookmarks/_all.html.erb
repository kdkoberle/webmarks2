<% @bookmarks.each do |bookmark| %>
  <tr>
    <td><%= bookmark.username %></td>
    <td><%= bookmark.name %></td>
    <td><%= bookmark.url %></td>
    <td><%= bookmark.date_saved %></td>
    <td><%= link_to 'Show', bookmark %></td>
  </tr>
<% end %>
</table>